        <Phase>
          <Name>Drop Off #1</Name>
          <Duration>60</Duration>
        </Phase>
        <Phase>
          <Name>To Astro #2</Name>
          <Duration>120</Duration>
        </Phase>
        <Phase>
          <Name>Drop Off #1</Name>
          <Duration>120</Duration>
